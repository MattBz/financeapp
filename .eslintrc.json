{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "globals": {
    "beforeEach": false,
    "describe": false,
    "document": false,
    "expect": false,
    "it": false,
    "jest": false,
    "test": false,
    "window": false
  },
  "rules": {
    "comma-dangle": "off",
    "max-len": ["error", {"code": 120, "ignoreComments": true}],
    "react/require-default-props": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }]
  }
}
